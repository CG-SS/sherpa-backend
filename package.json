    "eslint-plugin-prettier": "^4.1.0",
    "express": "^4.18.1",
    "prisma": "^3.15.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/cli": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
