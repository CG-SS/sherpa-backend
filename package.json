    "@nrwl/linter": "^14.3.6",
    "@nrwl/node": "14.3.6",
    "@nrwl/workspace": "14.3.6",
    "@swc/jest": "0.2.20",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
